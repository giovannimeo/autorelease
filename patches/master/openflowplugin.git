         <!-- thirdparty -->
         <dependency>
-          <groupId>org.opendaylight.controller.thirdparty</groupId>
+          <groupId>org.openexi</groupId>
           <artifactId>nagasena</artifactId>
           <version>${exi.nagasena.version}</version>
         </dependency>
         <dependency>
-          <groupId>org.opendaylight.controller.thirdparty</groupId>
+          <groupId>org.openexi</groupId>
           <artifactId>nagasena-rta</artifactId>
           <version>${exi.nagasena.version}</version>
         </dependency>
